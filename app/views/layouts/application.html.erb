<!DOCTYPE html>
<html>
<head>
  <title>Sendero</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if current_user %>
  <%= "#{current_user.email}" %> | 
  <%= link_to "My Projects", projects_path %> | 
  <%= link_to "All Routes", routes_path %> | 
  <%= link_to "Log out", destroy_user_session_path, method: :delete %><br>
<% else %>
  <%= link_to "Log in", new_user_session_path %><br>
  <%= link_to "Register", new_user_registration_path %><br>
<% end %>

<%= yield %>

</body>
</html>
